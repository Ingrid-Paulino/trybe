{
  "version": "0.2.0",
  "configurations": [{
    "name": "Launch via NPM",
    "request": "launch",
    "runtimeArgs": [
      "run-script",
      "test:dev"
    ],
    "runtimeExecutable": "npm",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "pwa-node"
  },
  {
    "console": "integratedTerminal",
    "internalConsoleOptions": "neverOpen",
    "name": "nodemon",
    "program": "${workspaceFolder}/src/index.ts",
    "request": "launch",
    "restart": true,
    "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/nodemon",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "pwa-node",
    "args": ["--ext", "ts", "--watch", "src", "--exec", "ts-node"]
  }
],
  "compounds": []
}